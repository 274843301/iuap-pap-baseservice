<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.yonyou.iuap</groupId>
	<artifactId>iuap-pap-baseservice</artifactId>
	<version>3.5.1-SNAPSHOT</version>
	<packaging>pom</packaging>

	  <properties>
	    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	    <java.version>1.8</java.version>
	    <iuap.module.version>3.1.0-RELEASE</iuap.module.version>
	    <eiap.module.version>3.5.1-RELEASE</eiap.module.version>
	    <pap.module.version>3.5.1-SNAPSHOT</pap.module.version>
	    <spring.verison>4.0.5.RELEASE</spring.verison>
	  </properties>

	<dependencyManagement>
		<dependencies>
		</dependencies>
	</dependencyManagement>

	<repositories>
		<repository>
			<id>nexus</id>
			<name>Nexus</name>
			<url>http://maven.yonyoucloud.com/nexus/content/groups/public</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>

		<repository>
			<id>iUAP-Snapshots</id>
			<name>iUAP-Snapshots</name>
			<url>http://maven.yonyoucloud.com/nexus/content/repositories/iUAP-Snapshots/</url>
			<releases>
				<enabled>false</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>iUAP-Stagings</id>
			<name>iUAP-Stagings</name>
			<url>http://maven.yonyoucloud.com/nexus/content/repositories/iUAP-Stagings/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>iUAP</id>
			<name>iWeb</name>
			<url>http://maven.yonyoucloud.com/nexus/content/repositories/iUAP/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>central</id>
			<name>central</name>
			<url>http://maven.yonyoucloud.com/nexus/service/local/repositories/central</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
	</repositories>
	<pluginRepositories>
		<pluginRepository>
			<id>maven-net-cn</id>
			<name>Maven China Mirror</name>
			<url>http://maven.net.cn/content/groups/public/</url>
			<releases>
				<enabled>true</enabled>
			</releases>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</pluginRepository>
	</pluginRepositories>


	<modules>
		<module>persistence</module>
		<module>service</module>
		<module>controller</module>
		<module>entity</module>
    	<module>bpm</module>
        <module>attachment</module>
        <!--<module>iuap-mybatis-maven-plugin</module>-->
  </modules>
	<distributionManagement>
		<snapshotRepository>
			<id>iUAP-Snapshots</id>
			<name>iUAP-Snapshots</name>
			<url> http://maven.yonyou.com/nexus/content/repositories/iUAP-Snapshots/</url>
		</snapshotRepository>
		<repository>
			<id>iUAP-Stagings</id>
			<name>iUAP-Stagings</name>
			<url>http://maven.yonyou.com/nexus/content/repositories/iUAP-Stagings/</url>
		</repository>
	</distributionManagement>
</project>